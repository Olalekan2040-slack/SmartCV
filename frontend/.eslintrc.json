{
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "react-hooks/exhaustive-deps": "warn",
    "jsx-a11y/anchor-is-valid": "warn",
    "no-useless-escape": "warn",
    "import/no-anonymous-default-export": "warn"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  }
}
